{
  "name": "connection-manager-api",
  "version": "1.5.1",
  "description": "ModusBox Connection Manager API",
  "main": "src/index.js",
  "scripts": {
    "testX": "standardx --verbose | snazzy && TEST=true mocha",
    "test": "P12_PASS_PHRASE=test_pass_phrase TEST=true mocha",
    "wtftest": "P12_PASS_PHRASE=test_pass_phrase TEST=true wtfnode ./node_modules/.bin/_mocha",
    "prestart": "npm install && node ./src/print-env",
    "start": "P12_PASS_PHRASE=${P12_PASS_PHRASE:-so_secret_default} node src/index.js",
    "build": "babel src --out-dir ./dist --source-maps",
    "serve": "node ./dist/index.js"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "chai": "^4.2.0",
    "connect": "^3.2.0",
    "cookies": "^0.7.3",
    "cors": "^2.8.5",
    "express-winston": "^3.1.0",
    "form-data": "^2.3.3",
    "joi": "^14.3.1",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.3",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sinon": "^7.4.1",
    "snazzy": "^8.0.0",
    "soap": "^0.29.0",
    "sqlite3": "^4.0.8",
    "swagger-tools": "^0.10.4",
    "tmp-promise": "^1.0.5",
    "winston": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.1.4",
    "standardx": "^3.0.1"
  },
  "standardx": {
    "semistandard": true
  }
}
